\begin_inset LatexCommand \cite{BM}

\end_inset 


\layout Standard


\begin_inset Formula \begin{equation}
E_{GMR}=41A^{-1/3}\;[MeV]\label{emdr}\end{equation}

\end_inset 


\begin_inset Formula \begin{equation}
\Gamma_{GMR}=4.0\:[MeV]\label{wmdr}\end{equation}

