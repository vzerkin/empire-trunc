         },
         {
            "identifier": "kal14",
            "description": "kal: Edit sensitivity input ",
            "command": " code $file-inp.sen ",
            "working_directory": "$dir"
         },
         {
            "identifier": "kal15",
            "description": "kal: View/Edit sensitivity matrix ",
