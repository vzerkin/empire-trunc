                     "1",
                     "2",
                     "4",
                     "16",
                     "17",
                     "18",
                     "22",
                     "102",
                     "103",
                     "104",
                     "105",
                     "107",
                     "112",
                     "115",
                     "0"
                  ]
               }
            ]
         },
         {
            "identifier": "kal2",
            "description": "kal: dirariance plot",
            "command": "gnuplot $EMPIREDIR/util/kalman/corr.gp & ",
            "working_directory": "$dir"
         },
         {
            "identifier": "kal3",
            "description": "kal: Clean previous Kalman files",
            "command": " $EMPIREDIR/scripts/klean",
            "working_directory": "$dir"
         },
         {
            "identifier": "kal4",
            "description": "kal: Propagate Kalman output into Empire input (*.new)",
