            "form": [
               {
                  "variable": "$var1",
                  "question": "Select #: 1 (total), 2 (elastic), 4 (inelastic), 16 (n,2n), 17 (n,3n), 18 (fission), 22 (n,na), 102 (capture), 103 (n,p), 104 (n,d), 105 (n,t), 107 (n,a), 112 (n,pa), 115 (n,pd) ",
                  "options": [
                     "1",
                     "2",
                     "4",
                     "16",
                     "17",
                     "18",
                     "22",
                     "102",
                     "103",
                     "104",
                     "105",
                     "107",
                     "112",
                     "115"
                  ]
               }
            ]
         },
         {
            "identifier": "kal1",
            "description": "kal: run Kalman for: ",
            "command": "$EMPIREDIR/scripts/kalman  $file $var2 $mat $EXPDAT}",
            "working_directory": "$dir",
            "form": [
               {
                  "variable": "$var2",
