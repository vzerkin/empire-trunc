#!/bin/bash
echo c4zvv ZA MT file.c4 file.endf file.zvd
echo c4zvv 42100 102 mo100.c4 mo100.end mo100.zvd
echo $1 $2 $3 $4 $5
rm tmp.dat c4.zvd end.zvd $5
./c4dat.exe $1 $2 $3 tmp.dat
./datzvd.exe tmp.dat c4.zvd
./endzvd.exe $1 $2 $4 end.zvd
eecho #!zvview.exe >$5
cat c4.zvd >>$5
cat end.zvd >>$5
./zvv94l.exe $5

