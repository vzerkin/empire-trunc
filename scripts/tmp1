guizvv.tcl:            set script [file dirname [info script]]
guizvv.tcl:            set filename [file join $script [file tail $filename] ]
guizvv.tcl:            set description "file not found!"
guizvv.tcl:    set ::vTcl(images,$reference,description) $description
